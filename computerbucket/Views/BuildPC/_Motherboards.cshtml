@model PagedList.IPagedList<computerbucket.Motherboard_tbl>
@using PagedList.Mvc

<table id="tblMotherboards" class="table table-striped table-condensed table-hover" 
       style="table-layout: fixed; word-wrap: break-word">
    
    <thead>
        <tr>
            <th style="width:80%">@Html.DisplayNameFor(m => m.FirstOrDefault().title)</th>
            <th style="width:20%">@Html.DisplayNameFor(m => m.FirstOrDefault().qty_stock)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var motherb in Model)
        {
            <tr onclick="listParts('@motherb.motherboard_id');" style="cursor:pointer">
                <td>
                    @String.Format("{0:d}",motherb.title)
                </td>
                <td>
                    <label class="label label-info pull-right">@motherb.qty_stock</label>
                </td>
            </tr>
        }
    </tbody>
</table>