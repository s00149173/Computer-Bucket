@model IEnumerable<computerbucket.PreBuildPC>

@{
    ViewBag.Title = "PreBuildComputers";
}
    <h2>@ViewBag.Title</h2>    
    
<div class="container">
    @foreach (var item in Model)
    {
        <div class="container">
            <div class="row">
                <h3>@item.PCType.TypeName</h3>
                @Html.Action("PreBuildComputer", new { id = item.PreBuildPCID })
            </div>
        </div>
        
        <br />
        <br />
        
    }

</div>